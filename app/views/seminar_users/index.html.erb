<p id="notice"><%= notice %></p>

<h1>Seminar Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seminar_users.each do |seminar_user| %>
      <tr>
        <td><%= link_to 'Show', seminar_user %></td>
        <td><%= link_to 'Edit', edit_seminar_user_path(seminar_user) %></td>
        <td><%= link_to 'Destroy', seminar_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

