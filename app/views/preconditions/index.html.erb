<p id="notice"><%= notice %></p>

<h1>Preconditions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @preconditions.each do |precondition| %>
      <tr>
        <td><%= link_to 'Show', precondition %></td>
        <td><%= link_to 'Edit', edit_precondition_path(precondition) %></td>
        <td><%= link_to 'Destroy', precondition, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Precondition', new_precondition_path %>
