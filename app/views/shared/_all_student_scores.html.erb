<div class="singleOption">
    <h3>Total Stars Earned: <strong><%= @total_stars %></strong></h3>
</div>
</br>

<div class="row">
  <div class="col-sm-6">
    <% if @learn_options && @learn_options.first %>
      <div class="singleOption lowSingOpt">
        <% suggestion = @learn_options.first %>
        <% if suggestion %>
          <% score = @student.objective_students.find_by(:objective_id => suggestion.id) %>
          <h3>Suggested objective for you to study next:</h3>
          <h4>&emsp;&emsp;<%= suggestion.name %>&emsp;&emsp; <small>Your Score:</small> <%= score.points %></h4>
        <% end %>
      </div>
    <% end %>
  </div>
  <div class="col-sm-6">
    <div class="singleOption lowSingOpt center">
      <h3><%= yield(:scores_label) %></h3><br/>
      <table class="marginTable">
        <% this_ss = @student.seminar_students.find_by(:seminar_id => @seminar.id) %>
        <% @seminar.objectives.order(:name).each do |objective| %>
            <tr>
              <td><%= objective.name %></td>
              <td><%= @student_scores.find_by(:objective => objective).points %></td>
            </tr>
        <% end %>
      </table>
      <br/>
    </div>
  </div>
</div>





