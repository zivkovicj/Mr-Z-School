<% provide(:title, 'Consultant Requests') %>

<%= render 'shared/back-link' %><br/>


<div class="row">
    <aside class="col-md-4">
        <section class="user_info">
            <h1>
                <strong><%= "#{@student.last_name_first}" %></strong>
                &nbsp;
                    <span class="btn-group">
                        <%= button_to( "Edit/Move Student", edit_student_path(@student), :method => :get,
                            :class => "btn btn-med btn-primary") %>
                    </span>
            </h1>
        </section>
    </aside>
</div>

<br/>

<% provide(:scores_label, "Student's Scores in All Objectives") %>
<% provide(:button_text, "Change Student Requests") %>

<div class="singleOption">
    <h2 class="add_key">Give Keys</h2>
    <table>
        <% @seminar.objectives.order(:name).each do |objective| %>
            <% this_os = objective.objective_students.find_by(:user => @student) %>
            <tr>
                <% @term = @seminar.term_for_seminar %>
                <%= render 'shared/keys/add_key_buttons', :this_os => this_os %>
                <td><%= objective.name %></td>
                <td><%= render 'shared/keys/list_of_keys', :this_os => this_os, :this_sem => @seminar %></td>
            </tr>
        <% end %>
    </table>
</div>
</br>

<%= render 'shared/teach_requests' %>

<%= render 'shared/all_student_scores' %>