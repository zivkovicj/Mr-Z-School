<p id="notice"><%= notice %></p>

<h1>Label Objectives</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @label_objectives.each do |label_objective| %>
      <tr>
        <td><%= link_to 'Show', label_objective %></td>
        <td><%= link_to 'Edit', edit_label_objective_path(label_objective) %></td>
        <td><%= link_to 'Destroy', label_objective, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Label Objective', new_label_objective_path %>
